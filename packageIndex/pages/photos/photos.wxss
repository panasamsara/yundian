/* pages/photos/photos.wxss */
page{
   width: 100%; 
  height: 100%;
  background-color: #eee;
}
.content{
  padding: 30rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.photo-list{
  width: 330rpx;
  height: 330rpx;
  margin-bottom: 30rpx;
}
.leftBtn{
  border: 1rpx solid #333;
  display: inline-block;
  width: 160rpx;
  text-align: center;
  border-top-left-radius: 10rpx;
  border-bottom-left-radius: 10rpx;
  margin-left:210rpx;

}
.btn-group{
  width: 100%;
  height: 60rpx;
  margin-top: 20rpx;
}
.rightBtn{
  border: 1rpx solid #333;
  display: inline-block;
  width: 160rpx;
  text-align: center;
  border-top-right-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}
.btn-active{
  background-color: #333;
  color: #fff;
}
.whitePic{
  width: 668rpx;
  height: 660rpx;
  margin-left: 40rpx;
  margin-top: 100rpx; 
}
.tips{
  color: #adadad;
  text-align: center;
}