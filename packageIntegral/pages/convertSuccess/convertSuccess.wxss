page{
  background:#f2f2f2;
}
.successTop{
  width: 100%;
  background: #fff;
}
.successTop .icon image{
  width: 42rpx;
  height: 42rpx;
  vertical-align: middle;
  margin-right: 20rpx;
}
.successTop .icon{
  padding: 100rpx 0 70rpx;
  font-size: 32rpx;
  color: #666;
  text-align: center;
}
.content{
  width: 100%;
  padding-left: 32rpx;
  box-sizing: border-box;
  padding-bottom: 32rpx;
  overflow: hidden;
  background:#f2f2f2;
}
.content .list{
  width: 327rpx;
  margin-right: 32rpx;
  margin-top: 32rpx;
  background: #fff;
  border-radius: 10rpx;
  box-sizing: border-box;
  float: left;
  height: 500rpx;
}
.content .list:first-child,.content .list:nth-child(2){
  margin-top:18rpx;
}
.list image{
  width: 100%;
  height: 327rpx;
}
.list .title{
  font-size: 30rpx;
}
.list .price{
  color: #fff;
  font-size: 28rpx;
}
.list .jifen{
  font-size: 45rpx;
  color: #fb452c;
  margin: 30rpx 20rpx 10rpx;
}
.com{
  width: 280rpx;
	height: 60rpx;
  border-radius: 30rpx;
  font-size: 30rpx;
  line-height: 60rpx;
  color: #fff;
  text-align: center;
  margin-left: 20rpx;
}
.list .exchange{
	background-color: #fb452c;
}
.list .deficiency{
  background-color: #DADADA;
}
.list text{
  font-size: 26rpx;
  margin-left: 20rpx;
}
.more{
  font-size: 30rpx;
  text-align: center;
  width: 680rpx;
  height: 80rpx;
  background: #fff;
  line-height: 80rpx;
  color: #666;
  margin-left: 34rpx;
  border-radius: 10rpx;
  margin-bottom: 28rpx;
}
.more image{
  width: 25rpx;
  height: 13rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}
.end{
    background: #f2f2f2;
    color: #999;
    font-size: 26rpx;
}
.viewBg{
  width: 100%;
  height: 327rpx;
  position: relative;
  color: #fff;
}
.bgTitle{
  background:rgba(0,0,0,0.4);
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 80rpx;
  padding:0 22rpx;
  box-sizing: border-box;
}
.recommend{
  font-size: 26rpx;
  color: #333;
  padding: 38rpx 0 0 32rpx;
}
.skip{
  padding-bottom: 88rpx;
}
.skip text{
  width: 180rpx;
  height: 58rpx;
  line-height: 58rpx;
  text-align: center;
  font-size: 28rpx;
  display: inline-block;
  border-radius: 28rpx;
}
.skip text:first-child{
  border: solid 2rpx #8d8d8d;
  color: #666666;
  margin-left: 160rpx;
}
.skip text:last-child{
  border: solid 2rpx #fb452c;
  color: #fb452c;
  margin-left: 70rpx;
}
