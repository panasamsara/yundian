page{
  background: #f2f2f2;
}
.nav{
  position: relative;
  width: 100%;
  height:220rpx;
}
.bg,.bgview{
  width: 100%;
  position: absolute;
  height:220rpx;
  display: block;
}
.integral{
 font-size: 60rpx;
 color: #fff;
 text-align: center;
 padding: 30rpx  0 20rpx 0;
}
.integral image{
  width: 23rpx;
  height: 25rpx;
}
.scroll-view_H{
  white-space: nowrap;
}
.detail-list-box{
  width:100%;
  padding:0 30rpx;
  box-sizing:border-box;
}
.list-left {
  float:left;
  width:352rpx;
  margin-top:25rpx;
  color:#333;
}
.list-right{
  float:right;
  width:290rpx;
  margin-top:25rpx;
}
.use-detail {
  text-align:right;
  top:-40rpx;
  font-size:24rpx;
  margin-bottom:10rpx;
  color: #777;
}
.use-time {
  text-align:right;
  font-size:30rpx;
  color:#333;
}
.list-card-name {
  font-size:26rpx;
  margin-bottom:10rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  color: #777;
}
.list-shop-name {
  font-size:30rpx;
  color:#333;
}
.content{
  border-radius: 10rpx;
  position: absolute;
  top: 140rpx;
  background: #fff;
  width: 702rpx;
  margin-left: 24rpx;
}
.content .detail-list {
  overflow: hidden;
  border-bottom: 2rpx solid #ddd;
  padding-bottom: 25rpx;
}
.content .detail-list:last-child{
  border-bottom:none;
}
.emptyImg image{
  width: 360rpx;
  height: 300rpx;
  display: block;
  margin: 236rpx auto 20rpx;
}
.emptyImg view{
  color: #999;
  font-size: 24rpx;
  text-align: center;
}
