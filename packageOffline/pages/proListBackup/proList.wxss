}
.cartcontrol-wrap{
  position: absolute;
  right: 0;
  bottom: 18px;
}
.cartcontrol{
  font-size: 0;
}
.cart-decrease{
  display:inline-block;
  padding:6px;
  opacity:1;
  transform:translate3d(0, 0, 0);
  width:28px;
  height:28px;
  background-repeat:no-repeat;
  background-size: contain;
  background-position:center;
  padding:0;
  float: left;
}
.cart-add{
  width:28px;
  height:28px;
  display: inline-block;
  padding: 6px;
  padding:0;
  line-height: 24px;
  font-size: 24px;
  color: rgb(0, 160, 220);
  background-repeat:no-repeat;
  background-size: contain;
  background-position:center;

}
.inner{
  display: inline-block;
  line-height: 24px;
  font-size: 24px;
  color: rgb(0, 160, 220);
  transition: all 0.4s linear;
  transform: rotate(0);
}
.inner.move-enter-active, .inner.move-leave-active{
  transition: all 0.4s linear;
}
.inner .move-enter, .inner.move-leave-active{
  opacity: 0;
  transform: translate3d(24px, 0, 0);
}
.inner{
  transform: rotate(180deg);
}
    
