/* 点击分享后出现的蒙层 */
.shareView{
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.4);
  z-index:100000;
}
.shareView .buttonGroup{
  width: 100%;
  height: 280rpx;
  position: absolute;
  bottom: 0;
  left: 0;
}
.shareView .buttonGroup button{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  background: red;
  text-align: center;
  background:rgba(255,255,255,1);
  font-size:32rpx;
  font-weight:500;
  color:rgba(51,51,51,1);
  border:none;
  border-top:0;
  border-bottom: 0;
}
.shareView .buttonGroup button::after{ border: none; }
.shareView .buttonGroup button.save_img{
  margin-bottom: 10rpx;
  border-top: 1rpx solid rgba(238,238,238,1);
}
/* 公共样式 */
.imageHtml{
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index:100000;
