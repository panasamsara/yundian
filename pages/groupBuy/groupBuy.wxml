<!--pages/groupBuy/groupBuy.wxml-->
<view class='container groupBuy'>
  <view wx:for="{{list}}" wx:key="index" class='list'>
    <image src='{{item.url}}' class='pic'></image>
    <view class='info'>
      <text class='title'>{{item.title}}</text>
      <view class='infoBox'>
        <view class='price'>
          <view class='priceBox'>
            <text class='newPrice'>￥{{item.newPrice}}</text>
            <view class='group'>{{item.groupNum}}人团</view>
          </view>
          <text class='oldPrice'>原价格{{item.oldPrice}}</text>
        </view>
        <view class='buyBox'>
          <view class='buy'>
            <navigator>去拼团</navigator>
          </view>
        </view>
      </view>
    </view>
  </view>
</view>