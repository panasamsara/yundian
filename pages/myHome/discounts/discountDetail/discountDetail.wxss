.discountDetail{
  height: 100%;
  z-index: 9999;
}
.list{
  width: 710rpx;
  background: #fff;
  margin: 0 20rpx;
  border-radius: 10rpx;
  overflow: hidden;
  margin-top:48px;
}
.list .shopIcon{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  position: absolute;
  left: 325rpx;
  top: 31rpx;
}
.list .shopname{
  margin-top: 80rpx !important;
}
.list .con{
  overflow: hidden;
  height: 306rpx;
  border-bottom: 2rpx dashed #ddd;
}
.list .con>view{
  margin-top: 18rpx;
  text-align: center;
}
.list .info{
  border-bottom: 2rpx dashed #ddd;
  color: #666;
  font-size: 28rpx;
  padding: 30rpx;
}
.list .info>view:first-child{
    color: #333;
    font-weight: bold;
}
.list .info>view{
  text-align: left;
  line-height: 50rpx;
  color: #666;
}
.cicrl{
  width: 30rpx;
  height: 30rpx;
  background: #f17168;
  position: absolute;
  border-radius: 50%;
 
}
.cicleLeft{
  left:6rpx;
  top:386rpx; 
}
.cicleRight{
  right: 6rpx;
  top:386rpx; 
}
.colory{
  background-color: #fb8b33;
}
.discount .con{
  height: auto;
  padding-bottom: 25rpx;
}
.discount .cicleLeft,.discount .cicleRight{
  top:426rpx; 
}
.discount .info{
  padding: 30rpx;
  overflow: hidden;
}
.discount .info view:first-child{
  color: #fb8b33;
  float: left;
}
.discount .info view:last-child{
  padding: 5rpx 20rpx;
  float: right;
  background: #fb8b33;
  color: #fff;
  border-radius: 5rpx;
}
.list .info button{
  float:right;
  background:#fb8b33;
  color:#fff;
  border-radius:5rpx;
  font-size: 28rpx;
  color: #fff;
  height: 46rpx;
  line-height: 46rpx;
}
.serial view{
  margin: 0 auto;
  width: 100%;
	border-radius: 4px;
  font-size: 32rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin: 40rpx 0 35rpx;
}
.serial view text{
  background-color: #eeeeee;
  padding: 10rpx 34rpx;
  display: block;
  margin: 0 auto;
  width: 60%;
  text-align: center;
  border-radius: 6rpx;
}
.serial image{
  display: block;
  width: 268rpx;
	height: 268rpx;
  margin: 0 auto;
}
.call{
  height: 80rpx;
  padding: 30rpx;
}
.canvas-box{
  position:fixed;
  top:999999rpx;
  left:0;
}
.call image:first-child{
  width: 42rpx;
  height: 42rpx;
  vertical-align: middle;
  float: left;
  margin-right: 24rpx;
}   
.call text{
  font-size: 26rpx;
  width: 545rpx;
  color: #333;
  display: block;
  float: left;
}
.call image:last-child{
  width: 32rpx;
  height: 42rpx;
  vertical-align: middle;
  float: left;
}
.discount .img{
  width: 112rpx;
  height: 112rpx;
  position: absolute;
  right:40rpx;
  top:178px;
}
.discount .noshare view:first-child{
  color: #999999;
}
.discount .noshare view:last-child{
  background: #999999;
}
.colorred{
  color: #f15b43;
}
.color999{
  color: #999;
}
.wrapList{
  padding-left: 20rpx;
  box-sizing: border-box;
  white-space: nowrap;
  display: flex;
  /* height: 230rpx; */
  overflow-x: scroll;
}
.wrapList .mylist{
  margin-right: 20rpx;
  width: 200rpx;
  float: left;
  padding-bottom:16rpx;
}
.wrapList .mylist image{
  width: 200rpx;
  height: 200rpx;
  display: block;

}
.wrapList .mylist view{
  font-size: 22rpx;
  text-align: center;
  margin-top: 10rpx;
}
.goods{
  font-size: 30rpx;
  color: #333;
  padding: 20rpx;
  font-weight: bold;
}
.wrapList .text-overflow{
  width: 200rpx;
}
.text-overflow{
  display:block;/*内联对象需加*/
  word-break:keep-all;/* 不换行 */
  white-space:nowrap;/* 不换行 */
  overflow:hidden;/* 内容超出宽度时隐藏超出部分的内容 */
  text-overflow:ellipsis;/* 当对象内文本溢出时显示省略标记(...) ；需与overflow:hidden;一起使用。*/
}
discount .info .text-overflow{
  width: 100%;
  text-align: center;
}
.discount .namecheck{
  margin-bottom: 34rpx;
}