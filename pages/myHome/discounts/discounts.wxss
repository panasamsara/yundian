.left{
  float: left;
}
.right{
  float: right;
}
.mr10{
  margin-right: 10rpx;
}
.bg{
  height: 232rpx;
  width: 100%;
}
.list{
  margin: 20rpx 26rpx;
  position: relative;
}
.con{
  width: 100%;
  position: absolute;
  top: 0;
}
.listTop{
  overflow: hidden;
  padding-bottom: 30rpx;
  color: #333;
  padding: 20rpx 0 30rpx 30rpx;
}
.listTop view:first-child{
  font-size: 30rpx;
  height: 54rpx;
  line-height: 54rpx;
  font-weight: bold;
}
.listTop view:last-child{
  font-size: 34rpx;
  font-weight: bold;
}
.listTop .left image{
  width: 54rpx;
  height: 54rpx;
  margin-right: 20rpx;
}
.time{
  color: #999;
  font-size: 28rpx;
  padding: 20rpx 0 20rpx 30rpx;
}