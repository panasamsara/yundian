.left{
  float: left;
}
.right{
  float: right;
}
.header{
  height: 416rpx; 
  width: 100%;
}
.headerToP .bgimg{
  width: 100%;
  height: 416rpx; 
  display: block;
}
.headerToP .top{
  position: absolute;
  top: 0;
  left: 50%;
  transform:translate(-50%);
}
.headerToP .top image{
  border-radius: 50%;
  display: block;
  width: 144rpx;
  height: 144rpx;
  margin: 66rpx auto 0;
}
.headerToP text{
  text-align: center;
  font-size: 34rpx;
  margin-top: 24rpx;
  margin-bottom: 40rpx;
  display: block;
  color: #fff;
}

.order{
  width: 100%;
  position:relative;
}
.onlineOrder{
  position: absolute;
  top: -102rpx;
  background: #fff;
  width: 690rpx;
  border-radius: 20rpx 20rpx 0 0;
  margin-left: 30rpx;
}

.onlineOrder .title image{
  width: 16rpx;
  height: 24rpx;
  vertical-align: middle;
  margin-top:20rpx;
}
.onlineOrder .title{
  line-height: 60rpx;
  overflow:hidden;
  font-size: 30rpx;
  padding:28rpx 30rpx 22rpx 30rpx;
}
.orderList{
  display: flex;
  border-top: 1px solid #eee;
  padding-top: 40rpx;
}
.orderList view{
  flex: 5;
  position: relative;
}
.msg,.msgtitle{
  position: absolute;
}
.orderList .msg{
  width: 33rpx;
  height: 34rpx;
  right: 50rpx;
  top: -17rpx;
}
.orderList .msgtitle{
  color: #fff;
  font-size: 16rpx;
  /* top: 10rpx; */
}
.orderList view image{
  width: 48rpx;
  height: 40rpx;
  display: block;
  margin: auto;
}
.orderList view text{
  font-size: 28rpx;
  text-align: center;
  display: block;
  width: 100%;
  color: #333;
  margin-top: 20rpx;
  margin-bottom: 30rpx;
  left:20rpx;
  top:-31rpx;
}
.shop{
  font-size: 50rpx;
}
.shoporder{
  overflow: hidden;
}
.shoporder view{
  width: 230rpx;
  float: left;
  padding-top: 60rpx;
}
.shoporder view image{
  width: 50rpx;
  height: 50rpx;
  display: block;
  margin: auto;
}
.shoporder view text{
  text-align: center;
  color: #666;
  font-size: 28rpx;
  width: 100%;
  display: block;
  margin-top: 20rpx;
}
.borderTop{
  border-top: 1px solid #eee;
}
.borderBottom{
  border-bottom: 1px solid #eee;
}
.paddingBottom{
  padding-bottom: 50rpx;
}
