page{
  height:100%;
}
.hidden{
  display: none !important;
}

.goods-title-view{
  border-bottom: 1px solid #eee;
  padding: 15px 10%;
  text-align: center;
  height: 26px;
}
.line-left,.line-right{
  display: inline-block;
  height: 1px;
  border-bottom: 1px solid #ADADAD;
  width: 30%;
  margin-bottom: 4px;
}
.good-title{
  padding: 0 15px;
  color: #333;
}
.goods{
    display: flex;
    width: 100%;
    /* overflow: hidden; */
    /* height: 100%; */
    /* padding-bottom: 35px; */
}
.menu-wrapper{
    flex: 0 0 100px;
    width: 100px;
    background: #f3f5f7;
    /* height: 100%; */
    /* padding-bottom: 50px; */
}
.menu-item{
  height:42px;
  width:80px;
  line-height:33px;
  padding:12px 12px 0;
  color:#999;
  font-size:0.8rem;
  text-align:center;
  overflow:hidden;
  vertical-align:middle;
}
.menu-item.current{
  color:#333;
  background: #fff;
}
.foods-wrapper{
  flex: 1;
}
.foods-item{
  display:flex;
  position:relative;
  margin:0 18px;
  padding-bottom:18px;
  height:60px;
  border-bottom:1px solid rgba(248,248,248,1);
  padding-top:18px;
}
.food-grouping:last-child .foods-item:last-child{
  border: none;
  margin-bottom: 0;
}
.foods-item .icon{ 
    display: inline-block;
    width: 60px;
    height: 60px;     
    flex: 0 0 57px;
    margin-right: 10px;
}
.foods-item .title{
  display: block;
  color: #2c2c2c;
  font-size: 0.8rem;
  width: 300rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box; /** 将对象作为伸缩盒子模型 **/
  -webkit-box-orient: vertical; /** 设置或检索伸缩盒对象的子元素的排列方式 **/
  -webkit-line-clamp: 1; /** 显示的行数 **/
}
.foods-item .content{
  position: relative;
  width: 270rpx;
  margin-bottom:-5rpx;
}
.foods-item .price{
  font-size: 34rpx;
  color: #FF5035;
  position: absolute;
  bottom: 0;
  left: 20rpx;
}
.foods-item .yangjiao{
  font-size: 26rpx;
  color: #FF5035;
  position: absolute;
  bottom: 5rpx;
}
.cartcontrol-wrap{
  position: absolute;
  right: 0;
  bottom: 18px;
}
.cartcontrol{
  font-size: 0;
}
.cart-decrease{
  display:inline-block;
  padding:6px;
  opacity:1;
  transform:translate3d(0, 0, 0);
  width:28px;
  height:28px;
  background-repeat:no-repeat;
  background-size: contain;
  background-position:center;
  padding:0;
  float: left;
}
.cart-add{
  width:28px;
  height:28px;
  display: inline-block;
  padding: 6px;
  padding:0;
  line-height: 24px;
  font-size: 24px;
  color: rgb(0, 160, 220);
  background-repeat:no-repeat;
  background-size: contain;
  background-position:center;

}
.inner{
  display: inline-block;
  line-height: 24px;
  font-size: 24px;
  color: rgb(0, 160, 220);
  transition: all 0.4s linear;
  transform: rotate(0);
}
.inner.move-enter-active, .inner.move-leave-active{
  transition: all 0.4s linear;
}
.inner .move-enter, .inner.move-leave-active{
  opacity: 0;
  transform: translate3d(24px, 0, 0);
}
.inner{
  transform: rotate(180deg);
}
    
.icon-remove_circle_outline{

}
.cart-count{
  display:inline-block;
  vertical-align:top;
  width:38px;
  padding-top:2px;
  line-height:24px;
  text-align:center;
  font-size:10px;
  color:rgb(0, 0, 0);
  line-height:20px;
  float:left;
  font-size:17px;
}


.icon-add_circle{

}

/*底部的购物车*/
.shopcart{
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 50;
    width: 100%;
    height: 48px;
}
.shopcart .content{
    display: flex;
    background: #fff;
    font-size: 0;
    color: rgba(255, 255, 255, 0.4);
}
.shopcart .content .content-left{
  flex: 1;
  border-top:1rpx solid #eee;
}
.shopcart .logo-wrapper{
  display: inline-block;
  vertical-align: top;
  position: relative;
  width: 128rpx;
  height: 128rpx;
  top: -40rpx;
  left: 20rpx;
  box-sizing: border-box;
  border-radius: 50%;

}
.shopcart .logo{
  width: 128rpx;
  height: 128rpx;
  position: relative;
  /* left: 15%;
  top: 15%;  */
  text-align: center;
  background-size: 100% 100%;
}
.shopcart .logo.highlight{
  background: #ff0037;
}
.shopcart{
  line-height: 44px;
  font-size: 24px;
  color: #80858a;
}
.shopcart.highlight{
  color: #fff;
}
.shopcart .num{
  position: absolute;
  top: -5rpx;
  left: 85rpx;
  /* right: 0; */
  width: 24px;
  height: 18px;
  line-height: 18px;
  text-align: center;
  border-radius: 8px;
  font-size: 9px;
  font-weight: 700;
  color: #fff;
  background: rgb(240, 20, 20);
  /* box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.4); */
}
.shopcart .price{
  display: inline-block;
  vertical-align: top;
  margin-top: 12px;
  margin-left:40rpx;
  line-height: 24px;
  padding-right: 12px;
  box-sizing: border-box;
  /* border-right: 1px solid rgba(255, 255, 255, 0.1); */
  font-size: 16px;
  font-weight: 700;
}
.shopcart .price.highlight{
  color: #ff0037;
  /* float:right; */

}
.shopcart .content-right{
  flex: 0 0 105px;
  width: 105px;
}
.content-right .pay{
  height: 48px;
  line-height: 48px;
  text-align: center;
  font-size: 12px;
  font-weight: 700;
  background: #2b333b;
}
.content-right .pay.enough{
  background: #00b43c;
  color: #fff;
}

