page{
  width: 100%;
  height: 100%;
  overflow: hidden;
  color: #333;
}
.left{
  float: left;
}
.right{
  float: right;
}
.scanBlock {
  width: 690rpx;
  height: 100rpx;
  background-image: linear-gradient(90deg, #fa973f 0%, #fa4c47 100%),
                      linear-gradient(#e00019, #e00019);
  background-blend-mode: normal, normal;
  border-radius: 47px;
  font-size: 32rpx;
  color: #fff;
  text-align: center;
  line-height: 100rpx;
  position: fixed;
  bottom: 50rpx;
  left: 30rpx;
}
.sys{
  width: 40rpx;
	height: 40rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
.bgNull,.wrap{
  width: 100%;
  height: 100%;
}
.wrapList{
  position: absolute;
  top: 580rpx;
  text-align: center;
  font-size: 28rpx;
  color: #666;
  width: 100%;
}
.bottomStys{
  position: absolute;
  bottom: 0;
  background: #fff;
  height: 140rpx;
  width: 100%;
}
.bottomStys .scanBlock{
  bottom: 20rpx;
  height: 90rpx;
  line-height: 90rpx;
}
.content{
  margin-top: 20rpx;
  height: 445rpx;
  overflow-y: scroll;
}
.child{
  margin-left: 40rpx;
  margin-top: 20rpx;
	height: 130rpx;
	background-color: #ffffff;
	box-shadow: 0px 2px 3px 0px 
		rgba(0, 0, 0, 0.1);
	border: solid 2px #f9f9f9;
  position: relative;
}
.child .logo{
  width: 82rpx;
  height: 82rpx;
  position: absolute;
  left: -40rpx;
  border-radius: 50%;
  top: 20rpx;
}
.child .left{
  margin-left: 70rpx;
}
.child .left view:first-child{
  font-size: 32rpx;
  color: #333;
  margin-top: 20rpx;
  height: 90rpx;
  line-height: 90rpx;
}
.child .right{
  width: 18rpx;
  height: 30rpx;
  margin-top: 50rpx;
  margin-right: 36rpx;
}
.wrapShop{
  margin: 0 30rpx;
}