.content{
  position: absolute;
  background: #f8f8f8;
  height: 100%;
  width: 100%;
}
.detail-list{
  padding-bottom: 100rpx;
}
.content .detail{
  background: #fff;
  margin:40rpx 30rpx;
  height: 100%;
  border-radius: 5px;
  box-shadow: 1px 1px 10px #ccc;
}
.content .detail .detail-top{
  color:#323232;
  font-size:30rpx;
  height:auto;
  padding:31rpx;
  overflow:hidden;
}
.content .detail .back{
  width: 30rpx;
  height: 48rpx;
  float: right;
  vertical-align: middle;
  margin-top:5px;
}
.content .detail .back image{
  display:block;
  width:80%;
  height:80%;
}
.content .check-box{
  display: inline-block;
}
.goods{
  height:320rpx;
  overflow:hidden;
  border-top:1rpx solid #e5e5e5;
  position: relative;
}
.goods .inner{
  position: absolute;
}
.goods .inner.txt{
  background-color: #fff;
  z-index: 5;
  transition: left0.2s ease-in-out;
  padding:0;
  left: 0;
  overflow:hidden;
  height:auto;
  display:flex;
  bottom:0;
  top:0;
  width:100%;
}
.goods .inner.delete{
  background-color:#e64340;
  width:180rpx;
  text-align:center;
  z-index:4;
  right:0;
  color:#fff;
  bottom:0;
  vertical-align:middle;
  top:0;
  line-height:320rpx;
}
.goods .good-left{
  width: 50%;
  float: left;
  position:relative;
  padding:15px 0 15px 15px;
  flex:1;
}
.goods .item-select{
  width: 40rpx;
  height: 40rpx;
  float: left;
  position: absolute;
  top:50%;
  margin-top:-20rpx;
  left:31rpx;
}
.goods .good-img{
  display:inline-block;
  width:240rpx;
  height:240rpx;
  border-radius:5px;
  margin-left:60rpx;
  margin-top:10rpx;
}
.goods .right-info{
  width: 50%;
  float: right;
  font-size: 30rpx;
  padding:31rpx;
}
.right-info text:first-child{
  display: block;
  margin-bottom: 10rpx;
}
.right-info text:nth-child(2){
  background: #f3f3f3;
  color:#989898;
  padding:5rpx 10rpx;
  margin-bottom:10rpx;
}
.right-info .detail-down{
  margin-top:15rpx;
  height:auto;
  overflow:hidden;
}
.right-info .detail-down text{
  display:inline-block;
  margin:0;
}
.right-info .detail-down text:first-child{
  font-size: 38rpx;
  color:#f57a92;
  display: block
}
.right-info .detail-down text:last-child{
  font-size: 38rpx;
  color:#969696;
  background: none;
  float: right;
}
.account-box{
  position: fixed;
  bottom: 0;
  width: 100%;
  font-size: 30rpx;
  border-top:1px solid #e5e5e5;
  background: #fff;
  z-index:33;
}
.account-box .all{
  padding: 10rpx 40rpx;
  display: inline-block;
  height: 70rpx;
  line-height:70rpx;
}
.account-box .sum-info{
  float: right;
  padding: 10rpx 40rpx 5rpx;
}
.account-box .sum-info .sum-box{
  height: 70rpx;
  line-height:70rpx;
  float:left;
  margin-right: 15px;
}
.account-box .sum-info .sum-box,.sum-box .sum-price{
  display: inline-block;
}
.sum-box .sum-price{
  color:#f57a92;
}
.account-btn{
  display: inline-block;
  width: 154rpx;
  height: 70rpx;
  border-radius: 20px;
  line-height: 70rpx;
  padding: 0;
  font-size: 30rpx;
  color:#fff !important;
  background: -webkit-linear-gradient(left,  #fd712c , #fa492c); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right,  #fd712c , #fa492c); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right,  #fd712c , #fa492c); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right,#fd712c , #fa492c); 
}
.detail-title{
  display: inline-block;
  height: 48rpx;
  overflow: hidden;
  position: relative;
  line-height:48rpx;
}
.detail-title image{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top:50%;
  margin-top:-20rpx;
}
.detail-title text{
  margin-left: 60rpx;
}
.all{
  position: relative;
}
.all .item-select{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  margin-top:-20rpx;
  top:50%;
}
.all text{
  margin-left: 50rpx;
}
.delete-box{
  background-color: #e64340;
  color: #ffffff;
  float: right;
  height: 100%;
  width: 80px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.stepper{
  display: inline-block;
  text-align: center;
  border:1rpx solid #ccc;
  border-radius:5px;
  margin:15rpx auto 0;
}
.stepper .minus{
  float: left;
  padding: 0 20rpx;
  color: #969696 !important;
}
.stepper input{
  width: 100rpx;
  float: left;
  text-align: center;
  border-right: 1rpx solid #ccc;
  border-left: 1rpx solid #ccc;
}
.stepper .add{
  padding: 0 20rpx;
  vertical-align: middle;
}
.top-control{
  position: fixed;
  width: 100%;
  padding:10rpx 0;
  top:0;
  background: #fff;
  z-index: 33;
  text-align: right;
  border: 1rpx solid #e5e5e5;
  border-left: none;
  border-right: none;
  box-shadow:1px 1px 10px #ccc;
}
.top-control button{
  display:inline-block;
  width:154rpx;
  height:70rpx;
  border-radius:20px;
  line-height:70rpx;
  padding:0;
  font-size:30rpx;
  color:#f9422b;
  border:1rpx solid #f9422b;
  background: #fff;
  margin-right: 30rpx;
}
.del{
  display: inline-block;
  float: right;
}
.del .del-icon{
  width:40rpx;
  height:40rpx;
  margin-top:19rpx;
}
.all-del{
  display: inline-block;
  height: 40rpx;
  padding-bottom:15rpx;
}
.all-del image{
  width:35rpx;
  height:35rpx;
  vertical-align:middle;
}