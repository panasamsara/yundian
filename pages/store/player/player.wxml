<!--pages/store/player/player.wxml-->
<view class='container player'> 
  <view wx:for="{{list}}" wx:key="index" class='list'>
    <image src='{{item.url}}' class='banner'></image>
    <view class='title'>
      <text>{{item.title}}</text>
      <image src='../images/yundian_spxq_xuanzhong.png' class='titleBorder'></image>
    </view>
    <view class='text'>
      <text>{{item.text}}</text>
    </view>
  </view>
</view>
